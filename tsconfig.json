{
  "compileOnSave": true,
  "compilerOptions": {
    // "strictNullChecks": true,
    "lib": [
      "es2015"
    ],
    "pretty": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "module": "commonjs",
    "target": "es2015",
    "jsx": "react",
   //  "skipLibCheck": true,
   
    "noEmitOnError": true,
    "noImplicitAny": true,
    "importHelpers": true,
    "noImplicitThis": true,
    "removeComments": false,
    "inlineSourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowJs": false,
    "rootDir": ".",
    "outDir": "./out"
  }
}
